+import sqlite3
+def generate_report(start_date, end_date, status):
+    conn = sqlite3.connect("app.db")
+    cursor = conn.cursor()
+    sql = "SELECT * FROM orders "
+    sql += "WHERE created_at >= '" + start_date + "' "
+    sql += "AND created_at <= '" + end_date + "' "
+    sql += "AND status = '" + status + "'"
+    cursor.execute(sql)
+    return cursor.fetchall()
